{
  "name": "selester_cp_express_backend",
  "version": "1.0.0",
  "description": "",
  "main": "babel.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "test": "jest --watchAll --runInBand",
    "test:non-seq": "jest --watchAll",
    "test:ci": "jest --ci --collectCoverage --reporters=default",
    "test:cover": "jest --collectCoverage",
    "start": "nodemon --exec babel-node -r ./node_modules/dotenv/config src/index.js",
    "build": "babel src -d dist",
    "serve": "node -r dotenv/config dist/index.js",
    "serve:prod": "set NODE_ENV=production&&node -r dotenv/config dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@selesterkft/express-logger": "^1.2.0",
    "@selesterkft/sel-db": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.5",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-restricted-globals": "^0.2.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
